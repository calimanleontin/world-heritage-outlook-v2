{
  "name": "cristiroma/drupal-boilerplate-8",
  "description": "Project template for Drupal 8 projects with composer",
  "type": "project",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "",
      "role": ""
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type"    : "package",
      "package" : {
        "name"    : "dinbror/blazy",
        "version" : "1.8.2",
        "type"    : "drupal-library",
        "dist"    : {
          "url"  : "https://github.com/dinbror/blazy/archive/1.8.2.zip",
          "type" : "zip"
        }
      }
    },
    {
      "type"    : "package",
      "package" : {
        "name"    : "desandro/imagesloaded",
        "version" : "4.1.1",
        "type"    : "drupal-library",
        "dist"    : {
          "url"  : "https://github.com/desandro/imagesloaded/archive/v4.1.1.zip",
          "type" : "zip"
        }
      }
    },
    {
      "type"    : "package",
      "package" : {
        "name"    : "enyoo/dropzone",
        "version" : "4.3.0",
        "type"    : "drupal-library",
        "dist"    : {
          "url"  : "https://github.com/enyo/dropzone/archive/v4.3.0.zip",
          "type" : "zip"
        }
      }
    },
    {
      "type"    : "package",
      "package" : {
        "name"    : "desandro/masonry",
        "version" : "4.2.0",
        "type"    : "drupal-library",
        "dist"    : {
          "url"  : "https://github.com/desandro/masonry/archive/v4.2.0.zip",
          "type" : "zip"
        }
      }
    },
    {
      "type"    : "package",
      "package" : {
        "name"    : "jackmoore/colorbox",
        "version" : "1.6.4",
        "type"    : "drupal-library",
        "dist"    : {
          "url"  : "https://github.com/jackmoore/colorbox/archive/1.6.4.zip",
          "type" : "zip"
        }
      }
    },
    {
      "type"    : "package",
      "package" : {
        "name"    : "kenwheeler/slick",
        "version" : "1.6.0",
        "type"    : "drupal-library",
        "dist"    : {
          "url"  : "https://github.com/kenwheeler/slick/archive/1.6.0.zip",
          "type" : "zip"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "harvesthq/chosen",
        "version": "1.6.1",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/harvesthq/chosen/releases/download/v1.6.1/chosen_v1.6.1.zip",
          "type": "zip"
        },
        "require": {
          "composer/installers": "^1.2.0"
        }
      }
    },
    {
      "type"    : "package",
      "package" : {
        "name"    : "adamwulf/columnizer",
        "version" : "1.6.2",
        "type"    : "drupal-library",
        "dist"    : {
          "url"  : "https://github.com/adamwulf/Columnizer-jQuery-Plugin/archive/1.6.2.zip",
          "type" : "zip"
        }
      }
    }
  ],
  "require": {
    "adamwulf/columnizer": "^1.6",
    "behat/mink": "^1.7",
    "behat/mink-browserkit-driver": "^1.3",
    "behat/mink-goutte-driver": "^1.2",
    "behat/mink-selenium2-driver": "^1.3",
    "behat/mink-zombie-driver": "^1.4",
    "composer/installers": "^1.2",
    "cweagans/composer-patches": "^1.6",
    "desandro/imagesloaded": "^4.1",
    "desandro/masonry": "^4.2",
    "dinbror/blazy": "^1.8",
    "drupal-composer/drupal-scaffold": "^2.2",
    "drupal/captcha": "^1.0@beta",
    "drupal/chosen": "^2.3",
    "drupal/colorbox": "^1.4",
    "drupal/config_ignore_keys": "^1.0",
    "drupal/config_split": "^1.0@beta",
    "drupal/core": "8.5.*",
    "drupal/devel": "^1.0@RC",
    "drupal/display_field_copy": "^1.0",
    "drupal/ds": "^3.1",
    "drupal/editor_advanced_link": "^1.4",
    "drupal/editor_file": "^1.2",
    "drupal/entity_print": "^2.1",
    "drupal/facets": "^1.1",
    "drupal/facets_pretty_paths": "1.x-dev",
    "drupal/features": "^3.8",
    "drupal/field_group": "^1.0@RC",
    "drupal/file_browser": "^1.1",
    "drupal/geofield": "^1.0@alpha",
    "drupal/geofield_map": "*",
    "drupal/google_analytics": "^2.3",
    "drupal/honeypot": "^1.27",
    "drupal/image_effects": "^1.0",
    "drupal/image_widget_crop": "^2.0",
    "drupal/imce": "^1.6",
    "drupal/inline_responsive_images": "^2.1",
    "drupal/lang_dropdown": "^2.0@beta",
    "drupal/linkit": "5.0-beta6",
    "drupal/menu_block": "^1.5",
    "drupal/menu_breadcrumb": "^1.6",
    "drupal/menu_link_attributes": "^1.0@beta",
    "drupal/metatag": "^1.7",
    "drupal/migrate_api": "^1.1",
    "drupal/migrate_plus": "^2.0",
    "drupal/migrate_tools": "^2.0",
    "drupal/paragraphs": "^1.3",
    "drupal/pathauto": "^1.3",
    "drupal/purge": "^3.0@beta",
    "drupal/recaptcha": "^2.3",
    "drupal/redirect": "^1.2",
    "drupal/reroute_email": "^1.0",
    "drupal/responsive_favicons": "^1.4",
    "drupal/search_api": "^1.9",
    "drupal/search_api_autocomplete": "^1.0@alpha",
    "drupal/search_api_solr": "^1.0",
    "drupal/search_api_sorts": "^1.0@beta",
    "drupal/search_autocomplete": "^1.1",
    "drupal/slick": "^1.0",
    "drupal/slick_views": "^1.0@RC",
    "drupal/smart_trim": "^1.1",
    "drupal/smtp": "^1.0@beta",
    "drupal/social_media": "^1.3",
    "drupal/spamspan": "^1.0@alpha",
    "drupal/stage_file_proxy": "^1.0@alpha",
    "drupal/string_field_formatter": "1.x-dev",
    "drupal/token": "^1.4",
    "drupal/varnish_purge": "^1.11",
    "drupal/views_accordion": "^1.1",
    "drupal/webform": "^5.0@beta",
    "drupal/xmlsitemap": "^1.0@alpha",
    "enyoo/dropzone": "^4.3",
    "harvesthq/chosen": "^1.6",
    "jackmoore/colorbox": "^1.6",
    "kenwheeler/slick": "^1.6",
    "webflo/drupal-finder": "^0.3.0",
    "webmozart/path-util": "^2.3"
  },
  "require-dev": {
    "consolidation/robo": "~1",
    "phpunit/phpunit": ">=4.8.28 <5"
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "classmap": [
      "scripts/composer/ScriptHandler.php"
    ]
  },
  "scripts": {
    "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
    "pre-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
    ],
    "pre-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
    ],
    "post-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ],
    "post-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ]
  },
  "extra": {
    "installer-paths": {
      "docroot/core": ["type:drupal-core"],
      "docroot/libraries/{$name}": ["type:drupal-library"],
      "docroot/modules/contrib/{$name}": ["type:drupal-module"],
      "docroot/profiles/contrib/{$name}": ["type:drupal-profile"],
      "docroot/themes/contrib/{$name}": ["type:drupal-theme"]
    },
    "patches-file": "composer.patches.json",
    "enable-patching": true
  }
}
