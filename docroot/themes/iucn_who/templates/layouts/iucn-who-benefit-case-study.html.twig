{%
set classes = [
'iucn-who-benefit-case-study',
]
%}

<div{{ attributes.addClass(classes) }}>
  <div class="container">
    <div class="row">
      <div class="col-sm-12">
        {{ content.image }}
      </div>
    </div>
    <div class="row">
      <div class="col-sm-8">
        {{ content.left }}
      </div>
      <div class="col-sm-4">
        {{ content.right }}
      </div>
    </div>
  </div>
  <div class="row">
  {{ content.content }}
  </div>
</div>
